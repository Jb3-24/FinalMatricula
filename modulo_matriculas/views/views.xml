<odoo>
    <data>

        <!--Vista Form Matricula Alumno-->


        <record id="view_form_matricula" model="ir.ui.view">
            <field name="name">view.form.ma.matricula</field>
            <field name="model">ma.matricula</field>
            <field name="arch" type="xml">
                <form  create="false">
                    <sheet>
                            <group>
                                <field name ="decano" groups="modulo_matriculas.res_groups_administrador"/>
                                <field name ="name" placeholder="Ingrese su nombre"/>
                                <field name ="cedula_alumno" placeholder="Ingrese su nro. de cédula"/>
                                <field name ="periodo_id" domain="[('estado', '=', True)]"/>
                                <field name="carrera_id"/>
                            </group>
                            <br></br>

                            <group>
                                <field name="calcular_valores"/>
                            </group>
                            <br></br>
                            <group>
                                <field name="grupo_socioeconomico_id" help="Seleccione el grupo socio económico que fue asignado por bienestar estudiantil" attrs="{'invisible': [('calcular_valores', '=', False)]}"/>
                            </group>
                            <br></br>
                            <group>    
                                <field name="costo_optimo_id" string="Régimen Académico" help="Seleccione el año más reciente en el que reprobó asignaturas" attrs="{'invisible': [('calcular_valores', '=', False)]}"/>
                            </group>
                            <br></br>
                        <hr/>
                        <notebook>
                            <page string="Datos de Tercera Matrícula">

                            <group>
                                <field name ="ciclo_materias_reprobadas_tercera" help="Seleccione los ciclos en los cuales tiene tercera matrícula" domain="[('carrera_id', '=', carrera_id)]"/>
                            </group>
                                <br></br>

                            <group>
                                <field name="asignaturas_reprobadas_tercera" help="Seleccione todas las asignaturas en las que tiene tercera matrícula" domain="[('ciclo_id', '=', ciclo_materias_reprobadas_tercera)]"/>
                            </group>
                            </page>
                            <page string="Datos de Segunda Matrícula">
                            <group>
                                <field name="ciclo_materias_reprobadas" help="Seleccione los ciclos en los cuales tiene segunda matrícula" domain="[('carrera_id', '=', carrera_id)]"/>
                            </group>
                                <br></br>
                            <group>
                                <field name="paralelo_ciclo_reprobar" help="Seleccione el paralelo en el cuál reprobó asignaturas" domain="[('ciclo_id', '=', ciclo_materias_reprobadas), ('periodo_id', '=', periodo_id)]"/>
                            </group>
                                <br></br>
                            <group>
                                <field name="asignaturas_reprobadas" help="Seleccione todas las asignaturas en las que tiene segunda matrícula" domain="[('ciclo_id', '=', ciclo_materias_reprobadas)]"/>
                            </group>

                            </page>
                            <page string="Datos de Primera Matrícula" help="Ingresar solo las asignaturas pendientes que tenga en su malla curricular">

                            <group>
                                <field name ="ciclo_materias_pendientes" help="Seleccione los ciclos en los cuales tiene asignaturas pendientes" domain="[('carrera_id', '=', carrera_id)]" />
                            </group>
                                <br></br>
                            <group>
                                <field name="asignaturas_pendientes" help="Seleccione todas las asignaturas pendientes que tenga" domain="[('ciclo_id', '=', ciclo_materias_pendientes)]"/>
                            </group>
                                <br></br>
                            <group>
                                <field name="validar_matricula_1_2" invisible="1"/>
                            </group>
                                <br></br>
                            <group>
                                <field name ="ciclo_matricular_especial" domain="[('carrera_id', '=', carrera_id)]"  attrs="{'invisible': ['|',('validar_matricula_1_2', '=', False),('matricular_mismo_ciclo', '=', True)]}"/>
                            </group>
                            </page>

                        </notebook>
                        <group><field name="matricular_mismo_ciclo"/></group>
                            <div class="d-flex justify-content-center">
                                <button style="float: center;"
                                    string="Simular Matrícula"
                                    type="object"
                                    name="botonmatricular"
                                    class="btn btn-primary btn-lg oe_highlight"/>
                            </div>
                        <hr/>
                        <br></br>

                            <group>
                                <field name="ciclo_matricular" help="Ciclo en el que se puede matrícular" readonly="1" attrs="{'invisible':[('ocultar_resultados', '=', False)]}"/>
                            </group>
                            <group>
                                <field name="asignaturas_tercera" help="Solo puede solicitar matrícula a esta asignatura por ser tercera matrícula" readonly="1" attrs="{'invisible':[('ocultar_resultados', '=', False)]}"/>
                            </group>
                            <group>
                                <field name="asignaturas_segunda" help="Asignaturas a las que puede solicitar segunda matrícula" readonly="1" attrs="{'invisible':[('ocultar_resultados', '=', False)]}"/>
                            </group>
                            <group>
                                <field name="asignaturas_primera" help="Asignaturas pendientes a las que puede solicitar matrícula" readonly="1" attrs="{'invisible':[('ocultar_resultados', '=', False)]}"/>
                            </group>
                            <group>
                                <field name="valores_pagar" string="Valores aproximados a pagar" help="Valor estimado a pagar por pérdida de gratuidad" readonly="1" attrs="{'invisible':[('ocultar_resultados', '=', False)]}"/>
                            </group>
                            <group>
                                <field name="ocultar_resultados" invisible="1"/>
                            </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!--Vista Tree Periodo Matricula Alumno-->
        <record id="view_tree_periodo_matricula_alumno" model="ir.ui.view">
            <field name="name">view.tree.ma.periodomatricula</field>
            <field name="model">ma.periodomatricula</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="fecha_inicio"/>
                    <field name="fecha_fin"/>
                    <field name="estado"/>
                    <button
