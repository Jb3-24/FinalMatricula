                            type="object"
                            name="botonmatricularse"
                            attrs="{'invisible':[('estado', '!=', True)]}"
                            class="btn btn-primary"/>
                </tree>
            </field>
        </record>

        <!--Vista Tree Asignatura Alumno-->
        <record id="view_tree_asignatura" model="ir.ui.view">
