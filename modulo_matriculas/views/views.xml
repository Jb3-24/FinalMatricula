            <field name="model">ma.periodomatricula</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                            <group>
                                <field name="name"/>
                                <field name="fecha_inicio"/>
                                <field name="fecha_fin"/>
                                <field name="estado"/>
                            </group>
                        <notebook>
                        </notebook>
                    </sheet>

                </form>
            </field>
        </record>


        <!--Vista Form Periodo Matrícula Gestor-->
        <record id="view_form_ciclo" model="ir.ui.view">
            <field name="name">view.form.ma.ciclo</field>
            <field name="model">ma.ciclo</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                            <group>
                                <field name="nombre_ciclo" invisible="1"/>
                                <field name="nombre_ciclo"/>
                                <field name="creditos"/>
                                <field name="n_asignaturas"/>
                                <field name="numero_ciclo"/>
                                <field name="carrera_id"/>
                                <field name="paralelo_ids"/>

                            </group>
                        <notebook>
                        </notebook>
                    </sheet>

                </form>
            </field>
        </record>

        <!--Vista Form Paralelo-->
        <record id="view_form_paralelo" model="ir.ui.view">
            <field name="name">view.form.ma.paralelo</field>
            <field name="model">ma.paralelo</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                            <group>
                                <field name="name"/>
                                <field name="periodo_id" string="Periodo Matrícula" domain="[('estado', '=', True)]"/>
                                <field name="carrera_id" string="Filtrar por Carrera" />
                                <field name="ciclo_id" domain="[('carrera_id', '=', carrera_id)]"/>
                            </group>
                            <group>
                                <notebook string="Horario">
                                <page string="Lunes">
                                <field name="horario_lunes" nolabel="1"/>
                                </page>
                                <page string="Martes">
                                <field name="horario_martes" nolabel="1"/>
                                </page>
                                <page string="Miércoles">
                                <field name="horario_miercoles" nolabel="1"/>
                                </page>
                                <page string="Jueves">
                                <field name="horario_jueves" nolabel="1"/>
                                </page>
                                <page string="Viernes">
                                <field name="horario_viernes" nolabel="1"/>
                                </page>
                                </notebook>
                            </group>
                    </sheet>
                </form>
            </field>
        </record>

